int rowIndex = Integer.parseInt(vars.get(&quot;rowIndex&quot;));

// Construct variable names using rowIndex
String sqlEmail = vars.get(&quot;email_&quot; + rowIndex); // email_1, email_2, etc.
String extractedEmail = vars.get(&quot;apiEmail&quot;); // Extracted email from API response
String firstName = vars.get(&quot;first_name_&quot; + rowIndex); // first_name_1, first_name_2, etc.

// Log the values for debugging
log.info(&quot;Row Index: &quot; + rowIndex);
log.info(&quot;SQL Email: &quot; + sqlEmail);
log.info(&quot;Extracted Email: &quot; + extractedEmail);
log.info(&quot;First Name: &quot; + firstName);

// Compare the emails
if (extractedEmail != null &amp;&amp; extractedEmail.equals(sqlEmail)) {
    // Store first name in JMeter property
    props.put(&quot;first_name&quot;, firstName);
    log.info(&quot;Emails match. First name stored: &quot; + firstName);
}</stringProp>
